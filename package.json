{
  "name": "aase-poc",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\" --names \"server,client\" --prefix-colors \"yellow,cyan\"",
    "dev:client": "react-router dev",
    "dev:server": "tsx watch server/index.ts",
    "build:client": "react-router build",
    "build:server": "tsc --project server/tsconfig.json",
    "lint": "eslint .",
    "typecheck": "react-router typegen && tsc",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "postinstall": "cd server && npm install && cd ..",
    "prepare": "husky"
  },
  "dependencies": {
    "@react-router/express": "^7.9.6",
    "@react-router/node": "^7.9.6",
    "@react-router/serve": "^7.9.6",
    "axios": "^1.13.2",
    "isbot": "^5.1.32",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.9.2"
  },
  "devDependencies": {
    "@react-router/dev": "^7.9.2",
    "@tailwindcss/vite": "^4.1.13",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.13",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.1.7",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
